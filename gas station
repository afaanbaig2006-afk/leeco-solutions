int canCompleteCircuit(int* gas, int gasSize, int* cost, int costSize) {
    int sum1=0,sum2=0;
    for(int i=0;i<gasSize;i++){
        sum1+=gas[i];
        sum2+=cost[i];

    }
    if(sum1<sum2){
        return -1;
    }
    int tank=0,start=0;
    for(int i=0;i<gasSize;i++){
        tank =tank+ gas[i]-cost[i];
        if(tank<0){
            tank =0;
            start = i+1;
        }

    }
    return start;

    
}
