int maxSubArray(int* nums, int numsSize) {
    int max = 0;
    for(int i=0;i<numsSize;i++){
        max+=nums[i];
    }
    int sum=0;
    for(int i=0;i<numsSize;i++){
        sum+=nums[i];
        if(sum>max){
            max = sum;
        }
        if(sum<0){
            sum=0;
        }
    }
    return max;
}
